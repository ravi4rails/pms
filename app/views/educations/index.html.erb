<p id="notice"><%= notice %></p>

<h1>Educations</h1>

<table>
  <thead>
    <tr>
      <th>Course name</th>
      <th>Institute name</th>
      <th>Yesr of completion</th>
      <th>Field of study</th>
      <th>More info</th>
      <th>Employee</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @educations.each do |education| %>
      <tr>
        <td><%= education.course_name %></td>
        <td><%= education.institute_name %></td>
        <td><%= education.yesr_of_completion %></td>
        <td><%= education.field_of_study %></td>
        <td><%= education.more_info %></td>
        <td><%= education.employee %></td>
        <td><%= link_to 'Show', education %></td>
        <td><%= link_to 'Edit', edit_education_path(education) %></td>
        <td><%= link_to 'Destroy', education, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Education', new_education_path %>
